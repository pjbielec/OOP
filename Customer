
public class Customer {

	private String name;
	private int age;
	private int money;

	public Customer(String name, int age, int money) {
		this.name = name;
		this.age = age;
		this.money = money;
	}

	@Override
	public String toString() {
		return "Here enters " + name + " age " + age + " with " + money + " zlotys in pocket";
	}

	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}

	public int getMoney() {
		return money;
	}

}
